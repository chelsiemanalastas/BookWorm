@model List<Category>

<h1>Category List</h1>

<table class="table table-boredered table-striped">
    <thead>
        <tr>
            <th>Category</th>
            <th>Order</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var obj in Model.OrderBy(u => u.Name))
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>